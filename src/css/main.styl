*
 box-sizing: border-box
 padding: o
 margin: 0

html, body
  height: 100%

.left
  left: 0

.right
  right: 0

#dive
  background-color: black
  height: 100%
  position: relative

  .holder
    display: inline-block
    position: absolute
    overflow: hidden
    height: 100%
    width: 50%
    top: 0

  video
    width: 120%
    position: absolute
    top: 50%
    transform: translateY(-50%)